.treeFrom {
  position: relative;
  height: 100%;
  overflow-y: scroll;
  .el-tree {
    position: relative;
    cursor: default;
    background: unset;
    color: #fff;
  }
  .custom-tree-node {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 12px;
    padding-right: 4px;
  }
}
